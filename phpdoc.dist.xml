<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor
        configVersion="3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="https://www.phpdoc.org"
>
    <title>Blocks</title>
    <paths>
        <output>build/docs</output>
    </paths>

    <setting name="template.color" value="deep-orange"/>

    <version number="3.0">
        <api format="php">
            <source dsn=".">
                <path>includes</path>
            </source>
            <ignore hidden="true" symlinks="true">
                <path>includes/class-block-cache.php</path>
            </ignore>
        </api>
    </version>
</phpdocumentor>